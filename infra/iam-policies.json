{
  "Version": "2012-10-17",
  "Statement": [
    {
      "Sid": "EKSCluster",
      "Effect": "Allow",
      "Action": [
        "eks:*",
        "ec2:*",
        "cloudformation:*",
        "iam:*",
        "autoscaling:*",
        "elasticloadbalancing:*"
      ],
      "Resource": "*"
    },
    {
      "Sid": "Lambda",
      "Effect": "Allow",
      "Action": [
        "lambda:*",
        "s3:*",
        "logs:*"
      ],
      "Resource": "*"
    }
  ]
}
